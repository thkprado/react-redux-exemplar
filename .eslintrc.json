{
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jquery": true,
    "mocha": true
  },
  "rules": {
    // "quotes": 0,
    // "no-trailing-spaces": 0,
    // "eol-last": 0,
    // "no-unused-vars": 0,
    // "no-underscore-dangle": 0,
    // "no-alert": 0,
    // "no-lone-blocks": 0,
    // "react/jsx-indent": 0,
    // "no-console": 0,
    "max-len": 0
  }
}
